p3 <- ggplot(tuni_data1, aes(x = exposure_time_s, y = survival, colour = water_type))+
   geom_jitter(width = 0.20, height = 0.15)
   
p3 + facet_grid(cols = vars(temperature_c))

p4 <- ggplot(tuni_data1, aes(x = exposure_time_s, y = survival))+
   geom_count(aes(size = after_stat(prop)), group = 1, alpha = 0.3)+
  geom_jitter(aes(colour = water_type), width = 0.20, height = 0.10)+
   scale_size_area(max_size = 15)
   
  
p4 + facet_grid(cols = vars(temperature_c))
   

